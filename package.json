{
    "name": "qapp-server",
    "version": "1.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "pm2": "pm2",
        "start": "cross-env DEBUG=*,-babel nodemon --exec babel-node ./src/index.js",
        "build:prod": "babel src --out-dir dist",
        "start:prod": "pm2 stop pm2.prod.config.js && pm2 start pm2.prod.config.js",
        "stop:prod": "pm2 stop pm2.prod.config.js",
        "build:stage": "babel src --out-dir dist",
        "start:stage": "pm2 stop pm2.stage.config.js && pm2 start pm2.stage.config.js",
        "stage": "git pull origin master && npm-run-all build:stage start:stage",
        "prod": "git pull origin master && npm-run-all build:prod start:prod"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "babel-preset-env": "^1.6.1",
        "bluebird": "^3.4.7",
        "body-parser": "^1.16.0",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.1",
        "cross-env": "^3.1.4",
        "crypto": "^1.0.1",
        "debug": "^2.6.0",
        "ensureDir": "^1.0.6",
        "express": "^4.14.1",
        "google-auth-library": "^2.0.2",
        "googleapis": "^36.0.0",
        "hashids": "^1.1.1",
        "jwt-simple": "^0.5.1",
        "limax": "^1.6.0",
        "moment": "^2.20.1",
        "mongoose": "^4.10.6",
        "mongoose-auto-increment": "^5.0.1",
        "mongoose-delete": "^0.3.4",
        "mongoose-sequence": "^4.0.1",
        "morgan": "^1.8.0",
        "node-env-file": "^0.1.8",
        "node-fetch": "^1.7.3",
        "npm-run-all": "^4.1.2",
        "passport": "^0.3.2",
        "passport-jwt": "^2.2.1",
        "passport-local": "^1.0.0",
        "request-promise": "^4.2.2",
        "revalidator": "^0.3.1",
        "sanitize-html": "^1.14.1",
        "uuid": "^3.2.1",
        "winston": "^2.3.1"
    },
    "devDependencies": {
        "babel-cli": "^6.22.2",
        "nodemon": "^1.18.9",
        "pm2": "^2.7.2"
    }
}
